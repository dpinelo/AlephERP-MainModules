 <querysource>
  <name>qryMain</name>
  <sql>SELECT 
  emp.nombre as empresa, 
  serv.nombre as servicio, 
  sum(linea.importetotal) as total,
  sum(linea.importetotal + (linea.importetotal * linea.iva / 100) ) as totalconiva
FROM 
  empresas as emp, 
  servicios as serv, 
  facturasprov as fp LEFT JOIN lineasserviciosfacturasprov as linea ON fp.id=linea.idfactura
WHERE 
  emp.id = fp.idempresa AND 
  serv.id=linea.idservicio AND
  fecha between &lt;? value("P_FECHA_INICIO") ?> AND &lt;? value("P_FECHA_FIN") ?> AND
  emp.id=&lt;? value("P_ID_EMPRESA") ?>
GROUP BY emp.nombre, serv.nombre
ORDER BY serv.nombre</sql>
 </querysource>
 <querysource>
  <name>qryTotales</name>
  <sql>SELECT 
sum(total) as total,
sum(totalconiva) as totalconiva
FROM (
SELECT 
  emp.nombre as empresa, 
  serv.nombre as servicio, 
  sum(linea.importetotal) as total,
  sum(linea.importetotal + (linea.iva * linea.importetotal / 100)) as totalconiva
FROM 
  empresas as emp, 
  servicios as serv, 
  facturasprov as fp LEFT JOIN lineasserviciosfacturasprov as linea ON fp.id=linea.idfactura
WHERE 
  emp.id = fp.idempresa AND 
  serv.id=linea.idservicio AND
  fecha between &lt;? value("P_FECHA_INICIO") ?> AND &lt;? value("P_FECHA_FIN") ?> AND
  emp.id=&lt;? value("P_ID_EMPRESA") ?>
GROUP BY emp.nombre, serv.nombre
) AS FOO
</sql>
 </querysource>
