 <querysource>
  <name>qryMain</name>
  <sql>SELECT emp.nombre as empresa, fp.codfactura, fp.nombre,
fp.cifnif, fp.fecha, fp.neto, fp.totaliva, fp.total, 
linea.descripcion, serv.nombre as servicio, linea.importetotal
FROM empresas as emp, servicios as serv, facturasprov as fp 
LEFT JOIN lineasserviciosfacturasprov as linea ON fp.id=linea.idfactura
WHERE emp.id = fp.idempresa AND serv.id=linea.idservicio AND
fecha between &lt;? value("P_FECHA_INICIO") ?> AND &lt;? value("P_FECHA_FIN") ?> and emp.id=&lt;? value("P_ID_EMPRESA") ?>
ORDER BY fecha, codfactura</sql>
 </querysource>
