 <querysource>
  <name>qryMain</name>
  <sql>select distinct
  alm.nombre as almacen, 
  articulo.nombre as articulo, 
  stocks.cantidad
from 
  almacenesubicaciones as alm, 
  articulos as articulo, 
  stocks
where 
  alm.id=stocks.idubicacion and 
  articulo.id=stocks.idarticulo
order by articulo.nombre, alm.nombre</sql>
 </querysource>
