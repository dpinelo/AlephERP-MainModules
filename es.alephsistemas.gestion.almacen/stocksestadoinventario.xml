 <querysource>
  <name>qryMain</name>
  <sql>SELECT emp.nombre as empresa, fp.codfactura, fp.nombre,
fp.cifnif, fp.fecha, fp.neto, fp.totaliva, fp.total, 
li.neto as linea_neto, li.iva as linea_iva, li.totaliva as linea_totaliva, 
li.totallinea as linea_totallinea, sf.descripcion as seriefacturacion
FROM empresas as emp, facturascli as fp 
LEFT JOIN
lineasivafacturascli as li ON fp.id = li.idfactura
LEFT JOIN
seriesfacturacion as sf ON fp.idserie = sf.id
WHERE emp.id = fp.idempresa AND
fecha between &lt;? value("P_FECHA_INICIO") ?> AND &lt;? value("P_FECHA_FIN") ?> and emp.id=&lt;? value("P_ID_EMPRESA") ?> and fp.idserie = &lt;? value("P_ID_SERIE") ?>
ORDER BY sf.descripcion, fecha, codfactura</sql>
 </querysource>
 <querysource>
  <name>qryEmpresa</name>
  <sql>SELECT emp.nombre as empresa, emp.logo
FROM empresas as emp
WHERE emp.id=&lt;? value("P_ID_EMPRESA") ?></sql>
 </querysource>
 <querysource>
  <name>qryTotales</name>
  <sql>SELECT sum(li.neto) as totalbaseimponible, li.iva as iva, sum(li.totaliva) as totaliva, 
sum(li.totallinea) as totalfactura
FROM empresas as emp, facturascli as fp 
LEFT JOIN
lineasivafacturascli as li ON fp.id = li.idfactura
LEFT JOIN
seriesfacturacion as sf ON fp.idserie = sf.id
WHERE emp.id = fp.idempresa AND
fecha between &lt;? value("P_FECHA_INICIO") ?> AND &lt;? value("P_FECHA_FIN") ?> and emp.id=&lt;? value("P_ID_EMPRESA") ?> and fp.idserie = &lt;? value("P_ID_SERIE") ?>
GROUP BY li.iva
ORDER BY li.iva</sql>
 </querysource>
 <querysource>
  <name>qryTotalFactura</name>
  <sql>SELECT sum(li.totallinea) as totalfactura
FROM empresas as emp, facturascli as fp 
LEFT JOIN
lineasivafacturascli as li ON fp.id = li.idfactura
LEFT JOIN
seriesfacturacion as sf ON fp.idserie = sf.id
WHERE emp.id = fp.idempresa AND
fecha between &lt;? value("P_FECHA_INICIO") ?> AND &lt;? value("P_FECHA_FIN") ?> and emp.id=&lt;? value("P_ID_EMPRESA") ?> and fp.idserie = &lt;? value("P_ID_SERIE") ?></sql>
 </querysource>
